#include <stdio.h>
int main(){
    char ch[100];
    printf("Enter the statement :");
    fgets(ch, sizeof(ch), stdin);
    for (int i=0;i<100;i++)
    {
        if (ch[i]>='A' && ch[i]<='Z')
        ch[i]+=32;
        else if (ch[i]>='a' && ch[i]<='z')
        ch[i]-=32;
    }
    printf(" %s",ch);
    return 0;
}
