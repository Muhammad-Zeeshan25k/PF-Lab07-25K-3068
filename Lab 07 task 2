#include <stdio.h>
int main(){
	int arr[10],a,count=0,b;
	for (int i=0;i<10;i++)
	{
		printf("Enter number in %d index :",i);
		scanf("%d",&b);
		arr[i]=b;
	}
	printf("Enter the number you want to search :");
	scanf("%d",&a);
	for (int j=0;j<10;j++)
	{
		if (a==arr[j]){
			count++;
		}
	}
	printf("The number %d occured %d times",a,count);
	return 0;
}

